   "source": [
    "n = 38\n",
    "ls = [True for _ in range(n+1)]\n",
    "prime_ls = []\n",
    "for i in range(2, n+1):\n",
    "    if ls[i] == True:\n",
    "        prime_ls.append(i)\n",
    "        j = i * 2\n",
    "        while j <= n:\n",
    "            ls[j] = False\n",
    "            j += i\n",
    "\n",
    "print(prime_ls)\n"
   ]
